{
  "name": "terraform-aws-lambda",
  "packageManager": "yarn@4.3.0",
  "scripts": {
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "fix": "prettier --write .",
    "build-docker": "docker build -t terraform-aws-lambda-app .",
    "run-docker": "docker run -p 3200:3200 terraform-aws-lambda-app",
    "offline": "sls offline --stage=offline --httpPort 3200"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.49.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.52.1",
    "@opentelemetry/instrumentation-aws-lambda": "^0.43.0",
    "@opentelemetry/resource-detector-aws": "^1.5.2",
    "@opentelemetry/sdk-node": "^0.52.1",
    "@opentelemetry/sdk-trace-node": "^1.25.1",
    "@opentelemetry/semantic-conventions": "^1.25.1",
    "@types/aws-lambda": "^8.10.143",
    "aws-lambda": "^1.0.7",
    "prettier": "^3.3.3",
    "serverless": "^3.38.0",
    "serverless-offline": "^13.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "devDependencies": {
    "@types/node": "^22.4.0"
  }
}
